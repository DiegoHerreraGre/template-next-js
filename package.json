    "next-recaptcha-v3": "^1.5.2",
    "postcss": "^8.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "tailwind-merge": "^3.3.0",
    "three": "^0.177.0",
    "uuid": "^11.1.0",
