{
  "name": "example-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "info": "node -v && npm -v && npx prisma -v && pm2 -v",
    "check": "npm run lint && prisma validate",
    "deploy": "npm install && npm run pm2:rebuild",
    "pm2:start": "npm run lint && npm run build && pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs ecosystem.config.js",
    "pm2:list": "pm2 list",
    "pm2:status": "pm2 status ecosystem.config.js",
    "add:component": "COMPONENT=$(node -p \"process.argv[1] || ''\" \"$component\") && if [ -z \"$COMPONENT\" ]; then read -p 'Nombre del componente: ' COMPONENT; fi && npx shadcn@latest add $COMPONENT",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },
  "engines": {
    "node": "22.16.0"
  },
